<?xml version="1.0" encoding="ISO-8859-1"?>
<exml:component xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config">
  <exml:import class="ext.Container"/>

  <exml:constant name="A_CONSTANT" value="One two three">
    <exml:description>
      This is some constant
    </exml:description>
  </exml:constant>

  <exml:constant name="B_CONSTANT" value="456"/>

  <exml:constant name="C_CONSTANT" value="{new Object()}" type="Object"/>

  <exml:constant name="D_CONSTANT" value="{new button()}" type="ext.config.button"/>

  <exml:constant name="E_CONSTANT" value="{new Container()}" type="ext.Component"/>

  <panel/>
</exml:component>