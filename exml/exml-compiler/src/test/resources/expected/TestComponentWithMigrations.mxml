<?xml version="1.0" encoding="UTF-8"?>
<!---
    This is a TestComponent with panel as baseclass.
    @see ext.Panel
  -->
<Panel          xmlns:fx="http://ns.adobe.com/mxml/2009"
                xmlns:cfg="testNamespace.config.*"
                xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config"
                title="I am inside a package!">
  <fx:Script><![CDATA[
    import ext.Component;

    public native function TestComponentWithMigrations(config:testPackage.TestComponentWithMigrations = null);

    [Bindable]
    public var propertyWithDefaultElementUsingConfig:ext.Component;
  ]]></fx:Script>
  <fx:Declarations>
    <cfg:TestComponentWithMigrations id="config"/>

    <!--- Test migration of default value. -->
    <Component id="propertyWithDefaultElementUsingConfig"/>

    <!--- Test migration of type. -->
    <Component id="propertyWithTypeMigration"/>
  </fx:Declarations>

    <items>
      <Label text="foo"/>
      <Component/>
      <APanel bar="test"/>
      <BPanel bar="test"/>
      <BPanel>
        <bar>here</bar>
      </BPanel>
      <APanel/>
      <BPanel/>
      <APanel>
      </APanel>
      <BPanel>
      </BPanel>
    </items>
  
</Panel>
