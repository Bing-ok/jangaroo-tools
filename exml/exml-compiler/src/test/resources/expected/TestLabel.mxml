<?xml version="1.0" encoding="UTF-8"?>
<!---This is a label.-->
<Label          xmlns:fx="http://ns.adobe.com/mxml/2009"
                xmlns:cfg="testNamespace.config.*"
                xmlns:exml="http://www.jangaroo.net/exml/0.8"
                xmlns="exml:ext.config"
                text="{config.propertyOne}">
  <fx:Script><![CDATA[
    public native function TestLabel(config:TestLabel = null);
  ]]></fx:Script>
  <fx:Declarations>
    <cfg:TestLabel id="config"/>

    <!--- The label text -->
    <fx:String id="propertyOne"/>
  </fx:Declarations>

</Label>
